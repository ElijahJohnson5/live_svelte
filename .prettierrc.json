{
    "plugins": ["prettier-plugin-svelte"],
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": false,
    "printWidth": 140,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "overrides": [{"files": "*.svelte", "options": {"parser": "svelte"}}]
}
